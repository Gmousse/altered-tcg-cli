{
  "name": "altered-tcg-cli",
  "version": "1.0.0",
  "description": "CLI to interact with altered tcg API on altered.gg",
  "type": "module",
  "main": "dist/main.js",
  "bin": "./dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "dev": "tsx src/main.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "~12.0.0",
    "mustache": "~4.2.0",
    "open": "~10.1.1",
    "pino": "~8.17.2",
    "ky": "^1.8.1",
    "date-fns": "~4.1.0",
    "pino-pretty": "~10.3.0"
  },
  "devDependencies": {
    "@types/mustache": "~4.2.6",
    "@types/node": "~22.14.1",
    "@types/pino": "~7.0.0",
    "@vitest/coverage-v8": "~3.1.1",
    "eslint": "~9.36.0",
    "jiti": "^2.4.2",
    "prettier": "~3.5.3",
    "tsc-alias": "^1.8.15",
    "tsx": "~4.19.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "~3.1.1"
  }
}
